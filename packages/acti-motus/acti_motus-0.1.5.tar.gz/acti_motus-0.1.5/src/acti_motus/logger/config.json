{
    "version": 1,
    "disable_existing_loggers": false,
    "filters": {
        "id": {
            "()": "acti_motus.logger.IdFilter"
        }
    },
    "formatters": {
        "console": {
            "format": "%(asctime)s | %(id)s | %(levelname)s | %(name)s | %(funcName)s | %(message)s",
            "datefmt": "%Y-%m-%d %H:%M:%S"
        }
    },
    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "level": "INFO",
            "formatter": "console",
            "stream": "ext://sys.stdout",
            "filters": [
                "id"
            ]
        }
    },
    "loggers": {
        "acti_motus": {
            "level": "INFO",
            "handlers": [
                "console"
            ],
            "propagate": false
        },
        "labda_accelerometers": {
            "level": "INFO",
            "propagate": false
        }
    },
    "root": {
        "level": "WARNING",
        "handlers": [
            "console"
        ]
    }
}