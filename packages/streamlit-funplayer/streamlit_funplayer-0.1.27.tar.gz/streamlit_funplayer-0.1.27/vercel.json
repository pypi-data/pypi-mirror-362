{
  "version": "0.1.27",
  "name": "funplayer",
  "builds": [
    {
      "src": "streamlit_funplayer/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "streamlit_funplayer/frontend/dist/$1"
    }
  ],
  "functions": {},
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}