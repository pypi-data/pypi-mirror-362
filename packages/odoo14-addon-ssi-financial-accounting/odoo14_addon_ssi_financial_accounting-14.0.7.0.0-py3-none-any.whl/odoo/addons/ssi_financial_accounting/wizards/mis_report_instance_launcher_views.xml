<odoo>
<record id="mis_report_instance_launcher_form_view" model="ir.ui.view">
    <field name="name">mis_report_instance_launcher_form</field>
    <field name="model">mis_report_instance_launcher</field>
    <field name="arch" type="xml">
        <form>
            <group>
                <field name="base_date"/>
            </group>
            <footer>
                <button name="action_open_report"
                    string="Open"
                    type="object"
                    class="oe_highlight"
                    />
                or
                <button string="Cancel" class="oe_link" special="cancel"/>
            </footer>
        </form>
    </field>
</record>
</odoo>
