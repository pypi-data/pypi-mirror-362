<odoo>
    <data noupdate="1">
        <record id="bank_statement_sequence" model="ir.sequence">
            <field name="name">Bank Statement Number</field>
            <field name="code">account.bank.statement</field>
            <field name="prefix">BS/%(month)s/%(range_y)s/</field>
            <field eval="1" name="number_next" />
            <field eval="1" name="number_increment" />
            <field name="padding" eval="6" />
            <field name="use_date_range" eval="1" />
            <field eval="False" name="company_id" />
        </record>
        <record id="cash_register_sequence" model="ir.sequence">
            <field name="name">Cash Register Number</field>
            <field name="code">account.bank.statement</field>
            <field name="prefix">CR/%(month)s/%(range_y)s/</field>
            <field eval="1" name="number_next" />
            <field eval="1" name="number_increment" />
            <field name="padding" eval="6" />
            <field name="use_date_range" eval="1" />
            <field eval="False" name="company_id" />
        </record>
    </data>
</odoo>
