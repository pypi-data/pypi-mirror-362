    <field name="category_id" ref="financial_accounting_report_module_category" />
    <field
            name="users"
            eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"
        />
</record>

<record id="mis_report_group" model="res.groups">
    <field name="name">MIS Report</field>
    <field name="category_id" ref="financial_accounting_report_module_category" />
    <field
            name="users"
            eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"
        />
</record>

<!-- Other -->
