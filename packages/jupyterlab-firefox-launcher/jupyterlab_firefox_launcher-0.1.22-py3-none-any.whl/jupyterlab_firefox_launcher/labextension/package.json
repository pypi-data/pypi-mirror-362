{
  "name": "jupyterlab-firefox-launcher",
  "version": "0.1.22",
  "description": "JupyterLab extension to launch Firefox in a tab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/vantagecompute/jupyterlab-firefox-launcher",
  "bugs": {
    "url": "https://github.com/vantagecompute/jupyterlab-firefox-launcher/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "style/index.js"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "packageManager": "yarn@1.22.22",
  "repository": {
    "type": "git",
    "url": "https://github.com/vantagecompute/jupyterlab-firefox-launcher.git"
  },
  "scripts": {
    "build": "jlpm build:lib && jlpm build:labextension:dev && node copy-package-json-to-labextension.cjs",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "build:prod": "jlpm clean && jlpm build:lib && jlpm build:labextension && node copy-package-json-to-labextension.cjs",
    "clean": "jlpm clean:lib && jlpm clean:labextension",
    "clean:all": "jlpm clean:lib && jlpm clean:labextension",
    "clean:labextension": "rimraf jupyterlab_firefox_launcher/labextension",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "install:extension": "jlpm build",
    "watch": "run-p watch:src watch:labextension",
    "watch:labextension": "jupyter labextension watch .",
    "watch:src": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^4.0.0",
    "@jupyterlab/apputils": "^4.0.0",
    "@jupyterlab/launcher": "^4.0.0",
    "@lumino/widgets": "^2.0.0"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^4.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "typescript": "~5.0.2"
  },
  "jupyterlab": {
    "extension": true,
    "outputDir": "jupyterlab_firefox_launcher/labextension",
    "_build": {
      "load": "static/remoteEntry.07939604cb7056f0118f.js",
      "extension": "./extension",
      "style": "./style"
    }
  }
}