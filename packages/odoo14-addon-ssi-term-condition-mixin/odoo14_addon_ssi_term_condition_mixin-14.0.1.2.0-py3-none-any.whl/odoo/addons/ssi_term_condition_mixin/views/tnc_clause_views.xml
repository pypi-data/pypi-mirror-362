<odoo>
<record id="tnc_clause_view_tree" model="ir.ui.view">
    <field name="name">tnc_clause - Tree</field>
    <field name="model">tnc_clause</field>
    <field name="arch" type="xml">
        <tree>
            <field name="sequence" widget="handle" />
            <field name="name" />
            <field name="title" />
            <field name="section_id" />
        </tree>
    </field>
</record>


<record id="tnc_clause_view_form" model="ir.ui.view">
    <field name="name">tnc_clause - Form</field>
    <field name="model">tnc_clause</field>
    <field name="arch" type="xml">
        <form>
            <group name="group1" colspan="4" col="2">
                <field name="name" />
                <field name="title" />
                <field name="sequence" />
            </group>
            <notebook>
                <page name="content" string="Content">
                    <field name="raw_content" class="oe_edit_only" />
                    <field name="content" class="oe_read_only" />
                </page>
            </notebook>
        </form>
    </field>
</record>

<record id="tnc_clause_view_tree2" model="ir.ui.view">
    <field name="name">tnc_clause - Tree2</field>
    <field name="model">tnc_clause</field>
    <field name="arch" type="xml">
        <tree>
            <field name="name" />
            <field name="title" />
            <field name="section_id" />
        </tree>
    </field>
</record>


<record id="tnc_clause_view_form2" model="ir.ui.view">
    <field name="name">tnc_clause - Form2</field>
    <field name="model">tnc_clause</field>
    <field name="arch" type="xml">
        <form>
            <group name="group1" colspan="4" col="2">
                <field name="name" />
                <field name="title" />
                <field name="sequence" />
                <field name="section_id" />
            </group>
            <notebook>
                <page name="content" string="Content">
                    <field name="raw_content" class="oe_edit_only" />
                    <field name="content" class="oe_read_only" />
                </page>
            </notebook>
        </form>
    </field>
</record>
</odoo>
