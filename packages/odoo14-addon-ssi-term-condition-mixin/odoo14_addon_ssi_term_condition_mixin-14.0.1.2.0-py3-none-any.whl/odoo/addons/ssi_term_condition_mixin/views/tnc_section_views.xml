<odoo>
<record id="tnc_section_view_tree" model="ir.ui.view">
    <field name="name">tnc_section - Tree</field>
    <field name="model">tnc_section</field>
    <field name="arch" type="xml">
        <tree>
            <field name="sequence" widget="handle" />
            <field name="name" />
            <field name="title" />
        </tree>
    </field>
</record>


<record id="tnc_section_view_form" model="ir.ui.view">
    <field name="name">tnc_section - Form</field>
    <field name="model">tnc_section</field>
    <field name="arch" type="xml">
        <form>
            <group name="group1" colspan="4" col="2">
                <field name="name" />
                <field name="title" />
                <field name="sequence" />
                <field name="model_id" force_save="1" invisible="1" />
                <field name="tnc_object_id" invisible="1" />
            </group>
            <notebook>
                <page name="content" string="Content">
                    <field name="raw_content" class="oe_edit_only" />
                    <field name="content" class="oe_read_only" />
                </page>
                <page name="clause" string="Clauses">
                    <field name="clause_ids" />
                </page>
            </notebook>
        </form>
    </field>
</record>
</odoo>
