# AgentDK Examples Environment Configuration
# Copy this file to .env and fill in your actual values

# =============================================================================
# MySQL Database Configuration
# =============================================================================
# MySQL server host (default: localhost for local development)
MYSQL_HOST=localhost

# MySQL server port (default: 3306)
MYSQL_PORT=3306

# MySQL database name for AgentDK examples
MYSQL_DATABASE=agentdk_test

# MySQL username for AgentDK examples
MYSQL_USER=agentdk_user

# MySQL password for AgentDK examples
MYSQL_PASSWORD=agentdk_user_password

# =============================================================================
# AI Model Configuration
# =============================================================================
# OpenAI API Key (if using OpenAI models)
# OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key (if using Claude models)
# ANTHROPIC_API_KEY=your_anthropic_api_key_here
