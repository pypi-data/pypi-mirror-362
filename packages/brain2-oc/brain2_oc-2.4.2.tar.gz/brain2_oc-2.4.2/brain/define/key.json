{
	"_id": {
		"__type__": "string",
		"__regex__": "^[0-9a-f]{32}$",
		"__minimum__": 32,
		"__maximum__": 32
	},

	"_created": {
		"__type__": "timestamp",
		"__optional__": true
	},

	"_updated": {
		"__type__": "timestamp",
		"__optional__": true
	},

	"user": {
		"__type__": "tuuid"
	},

	"type": {
		"__type__": "string",
		"__options__": [ "forgot", "setup", "verify" ]
	}
}