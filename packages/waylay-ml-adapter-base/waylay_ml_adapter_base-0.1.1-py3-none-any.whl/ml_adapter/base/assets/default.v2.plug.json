{
    "$schema": "waylay:/registry/v2/schemas/plugs-manifest-01",
    "version": "0.0.1",
    "type": "sensor",
    "runtime": "plug-python3",
    "interface": {
      "states": [
        "INFERRED",
        "FAILED"
      ],
      "input": [
        {
          "name": "inputs",
          "dataType": "object",
          "mandatory": true
        },
        {
          "name": "id",
          "dataType": "string",
          "mandatory": false
        },
        {
          "name": "parameters",
          "dataType": "object",
          "mandatory": false
        },
        {
          "name": "outputs",
          "dataType": "object",
          "mandatory": false
        }
      ],
      "output": [
        {
          "name": "outputs",
          "dataType": "object",
          "mandatory": true
        },
        {
          "name": "id",
          "dataType": "string",
          "mandatory": false
        },
        {
          "name": "model_name",
          "dataType": "string",
          "mandatory": false
        },
        {
          "name": "model_version",
          "dataType": "string",
          "mandatory": false
        },
        {
          "name": "parameters",
          "dataType": "object",
          "mandatory": false
        }
      ]
    },
    "metadata": {
      "tags": [
        { "name": "MLAdapter", "color": "#4153ea" }
      ],
      "documentation": {
        "description": "",
        "states": [
          {
            "name": "INFERRED",
            "description": "The model inference succeeded."
          },
          {
            "name": "FAILED",
            "description": "The model inference failed."
          }
        ],
        "input": [
          {
            "name": "inputs",
            "description": "A list of named input tensors (Open Inference V2 protocol)."
          },
          {
            "name": "id",
            "description:": "The id of the inference request."
          },
          {
            "name": "parameters",
            "description:": "Additional parameters for the inference request."
          },
          {
            "name": "outputs",
            "description:": "A list of expected output tensor names."
          }
        ],
        "output": [
          {
            "name": "outputs",
            "description": "A list of named output tensors (Open Inference V2 protocol)."
          },
          {
            "name": "id",
            "description": "The id of the inference request."
          },
          {
            "name": "model_name",
            "description": "The name of the model used"
          },
          {
            "name": "model_version",
            "description": "The version of the model used"
          },
          {
            "name": "parameters",
            "description": "Additional parameters for the inference response."
          }
        ]
      }
    }
  }