{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug: agentrix run @h1deya/mcp-server-weather",
            "type": "python",
            "request": "launch",
            "module": "agentrix.cli",
            "args": [
                "run",
                "mcp-server-fetch"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src"
            },
            "justMyCode": false,
            "stopOnEntry": false
        },
