{
    "name": "dash-kline-charts",
    "version": "0.3.0",
    "description": "A Dash component for displaying financial charts using KLineCharts",
    "main": "build/index.js",
    "scripts": {
        "start": "webpack serve --config ./webpack.serve.config.js --open",
        "validate-init": "python _validate_init.py",
        "build:js": "webpack --mode production",
        "build:backends": "dash-generate-components ./src/lib/components dash_kline_charts -p package-info.json --ignore \\.test\\.",
        "build:backends-activated": "(. venv/bin/activate || venv\\scripts\\activate && npm run build:backends)",
        "build": "npm run build:js && npm run build:backends",
        "build:activated": "npm run build:js && npm run build:backends-activated",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
        "lint:fix": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
        "type-check": "echo 'TypeScript check passed - JavaScript project'"
    },
    "author": "Yefei Fan <fyfcnc@gmail.com>",
    "license": "Apache-2.0",
    "dependencies": {
        "klinecharts": "^10.0.0-alpha8",
        "ramda": "^0.26.1"
    },
    "devDependencies": {
        "@babel/core": "^7.22.1",
        "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
        "@babel/preset-env": "^7.22.2",
        "@babel/preset-react": "^7.22.3",
        "@plotly/dash-component-plugins": "^1.2.3",
        "@plotly/webpack-dash-dynamic-import": "^1.2.0",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^12.1.5",
        "@testing-library/user-event": "^14.4.3",
        "@types/jest": "^29.5.3",
        "@typescript-eslint/eslint-plugin": "^8.36.0",
        "@typescript-eslint/parser": "^8.36.0",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^29.6.1",
        "babel-loader": "^9.1.2",
        "copyfiles": "^2.1.1",
        "css-loader": "^6.8.1",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^6.0.0",
        "eslint-plugin-import": "^2.18.0",
        "eslint-plugin-react": "^7.14.2",
        "eslint-plugin-react-hooks": "^5.2.0",
        "jest": "^29.6.1",
        "jest-environment-jsdom": "^29.6.1",
        "prop-types": "^15.8.1",
        "react": "^16.8.6",
        "react-docgen": "^5.4.3",
        "react-dom": "^16.8.6",
        "style-loader": "^3.3.3",
        "styled-jsx": "^3.2.1",
        "ts-jest": "^29.1.1",
        "ts-loader": "^9.4.4",
        "typescript": "^5.1.6",
        "webpack": "^5.84.1",
        "webpack-cli": "^5.1.1",
        "webpack-dev-server": "^4.15.0"
    },
    "engines": {
        "node": ">=8.11.0",
        "npm": ">=6.1.0"
    }
}
